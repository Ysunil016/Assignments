package com.Spring.Services.Interface;

import com.Spring.Bean.Booking;
import com.Spring.Bean.UserRecord;

/**
 * 
 * @author sunil016, Interface holds all the functions related to Booking
 *         Service
 *
 */

public interface RatingInterface {

	// Passenger Rating Driver
	boolean rateDriver(Booking book, String from);

	// Passenger Rating Driver
	boolean ratePassenger(Booking book, String from);

	// Driver Getting Aggregated Rating
	float getAggregatedDriverRating(String driverId);

	// Driver Getting Aggregated Rating
	float getAggregatedDriverRating(String driverId);

}
